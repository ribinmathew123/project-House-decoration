                        <div class="page-pagination text-center" data-aos="fade-up" data-aos-delay="0">
                            <ul>
                                


                                <% if (prevPage) { %>
                                    <li>
                                        <a href="?page=<%= prevPage %>&sortby=<%= sortby %>">
                                            <i class="ion-ios-skipbackward"></i>
                                        </a>
                                    </li>
                                    <% } %>
                                        <% for (let i=1; i <=totalPages; i++) { %>
                                            <li <% if (currentPage===i) { %> class="active" <% } %>>
                                                    <a href="?page=<%= i %>&sortby=<%= sortby %>">
                                                        <%= i %>
                                                    </a>
                                            </li>
                                            <% } %>
                                                <% if (nextPage) { %>
                                                    <li>
                                                        <a href="?page=<%= nextPage %>&sortby=<%= sortby %>">
                                                            <i class="ion-ios-skipforward"></i>
                                                        </a>
                                                    </li>
                                                    <% } %>
                            </ul>
                        </div>
                        <!-- End Pagination -->
                    </div>
                    <!-- End Shop Product Sorting Section  -->
                </div>
            </div>
        </div>
