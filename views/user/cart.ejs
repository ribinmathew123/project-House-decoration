    <% if(cartList.length> 0){%>

    <div class="cart-section">
      <!-- Start Cart Table -->
      <div class="cart-table-wrapper" data-aos="fade-up" data-aos-delay="0">
        <div class="container">
          <div class="row">
            <div class="col-12">
              <div class="table_desc">
                <div class="table_page table-responsive">
                  <table>
                    <!-- Start Cart Table Head -->

                    <thead>
                      <tr>
                        <th class="product_thumb">Image</th>
                        <th class="product_name">Product</th>
                        <th class="product-price">Price</th>
                        <th class="product_quantity">Quantity</th>
                        <th class="product_total">Total</th>
                        <th class="product_remove">Delete</th>
                      </tr>
                    </thead>
                    <!-- End Cart Table Head -->
                    <tbody>
                      <!-- Start Cart Single Item-->
                      <% var totalAmount=0 %>

                      <% cartList.forEach((item)=>{%>

                      <tr>
                        <td class="product_thumb">
                          <img src="<%= item.product.image_url[0] %>" />
                        </td>

                        <td class="product_name"><%=item.product.name%></td>
                        <td class="product-price">$<%=item.product.price%></td>
                        <td class="product_quantity">
                          <label>Quantity</label>


                            <button class="increment-button btn" onclick="incrementCount('<%= item.userId%>','<%= item.cartItems.productId%>','<%=item.product.price%>')" data-product-id="<%= item.cartItems.productId%>">+</button>
                          <!-- <a type="button" href="/product/removecart?id=<%= item.cartItems.productId%>" ><i class="fa fa-trash-o"></i></a> -->
                  

                          
                        <p id="Quantity<%= item.cartItems.productId%>"><%= item.cartItems.qty %></p>
                        <button type="submit"onclick="decrementCount('<%= item.userId%>','<%= item.cartItems.productId%>','<%=item.product.price%>')" class="decrement-button" data-product-id="<%= item.product._id %>">-</button>
                      </td>


                        <% var price = item.cartItems.qty * item.product.price %>
                        <% totalAmount += price %>
                        <% console.log(totalAmount) %>
                    


                        <td  class="product_total">$<span id="total-price<%= item.cartItems.productId%>"  >
                          <%= price%>
                        </span> 
                       </td>
                        <td class="product_remove">
                          <a
                            type="button"
                            href="/product/removecart?id=<%= item.cartItems.productId%>"
                            ><i class="fa fa-trash-o"></i
                          ></a>
                        </td>
                      </tr>
                      <!-- End Cart Single Item-->
                      <!-- Start Cart Single Item-->

                      <%})%>
                    </tbody>
                  </table>
                </div>
                <div class="cart_submit">
                  <a
                    href="/product-display"
                    class="btn btn-md btn-golden"
                    type="submit"
                    >update cart</a
                  >
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
      <!-- End Cart Table -->

      <!-- Start Coupon Start -->
      <div class="coupon_area">
        <div class="container">
          <div class="row">
            <div class="col-lg-6 col-md-6">
              <div
                class="coupon_code left"
                data-aos="fade-up"
                data-aos-delay="200"
              >
                <h3>Coupon</h3>
                <div class="coupon_inner">
                  <p>Enter your coupon code if you have one.</p>
                  <input class="mb-2" placeholder="Coupon code" type="text" />
                  <button type="submit" class="btn btn-md btn-golden">
                    Apply coupon
                  </button>
                </div>
              </div>
            </div>
            <div class="col-lg-6 col-md-6">
              <div
                class="coupon_code right"
                data-aos="fade-up"
                data-aos-delay="400"
              >
                <h3>Cart Totals</h3>
                <div class="coupon_inner">
                  <div class="cart_subtotal">
                    <p>Subtotal</p>
                    <p class="cart_amount">$ <span id="total-amount1"> <%= totalAmount%></span></p>
                  </div>
                  <div class="cart_subtotal">
                    <p>Shipping</p>
                    <p class="cart_amount"><span>Free</span> $00.00</p>
                  </div>
                  <a href="#">Calculate shipping</a>

                  <div class="cart_subtotal">
                    <p>Total</p>
                    <p class="cart_amount">$ <span id="total-amount2"><%= totalAmount%></span></p>
                  </div>
                  <div class="checkout_btn">
                    <a href="#" class="btn btn-md btn-golden"
                      >Proceed to Checkout</a
                    >
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
      <!-- End Coupon Start -->
    </div>
    <!-- ...:::: End Cart Section:::... -->

