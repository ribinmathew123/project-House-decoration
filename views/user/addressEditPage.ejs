                                                 <form action="/update-address/<%= address._id %>" class="user" method="POST">
                                                    <div class="form-group row">
                                                        <div class="col-sm-6 input-control">
                                                            <label class="form-label">Full name*</label>
                                                            <input type="text" class="form-control form-control-user"
                                                                id="username" name="name" required value="<%= address.Fullname %>"/>
                                                            <p class="valid_error"></p>
                                                        </div>
                                                        <div class="col-sm-6 input-control">
                                                            <label class="form-label">Company/Shop</label>
                                                            <input type="text" class="form-control form-control-user"
                                                                id="" name="company"  required value="<%= address.company %>"  />
                                                            <p class="valid_error"></p>
                                                        </div>
                                                    </div>
                                                    <div class="form-group row">
                                                        <div class="col-sm-6 input-control">
                                                            <label class="form-label">Email*</label>
                                                            <input type="email" class="form-control form-control-user"
                                                                id="emailAddress" name="email" required  required value="<%= address.email%>"/>
                                                            <p class="valid_error"></p>
                                                        </div>
                                                        <div class="col-sm-6 input-control">
                                                            <label class="form-label">Mobile*</label>
                                                            <input type="tel" class="form-control form-control-user"
                                                                id="" name="mobile" pattern="[5-9]{1}[0-9]{9}"
                                                                required  required value="<%= address.mobile %>"/>
                                                            <p class="valid_error"></p>
                                                        </div>
                                                    </div>
                                                    <div class="form-group row">
                                                        <div class="col-sm-6 input-control">
                                                            <label class="form-label">Country*</label>
                                                            <input type="text" class="form-control form-control-user"
                                                                id="" name="country" required required value="<%= address.countryname %>"/>
                                                            <p class="valid_error"></p>
                                                        </div>
                                                        <div class="col-sm-6 input-control">
                                                            <label class="form-label">State*</label>
                                                            <input type="text" class="form-control form-control-user"
                                                                id="" name="state" required required value="<%= address.state%>"/>
                                                            <p class="valid_error"></p>
                                                        </div>
                                                    </div>
                                                    <div class="form-group row">
                                                        <div class="col-sm-6 input-control">
                                                            <label class="form-label">City/Town*</label>
                                                            <input type="text" class="form-control form-control-user"
                                                                id="" name="town" required  required value="<%= address.city%>"/>
                                                            <p class="valid_error"></p>
                                                        </div>
                                                        <div class="col-sm-6 input-control">
                                                            <label class="form-label">Postal Code/ZIP*</label>
                                                            <input type="tel" class="form-control form-control-user"
                                                                id="" name="zip" pattern="[0-9]{4,7}" required required value="<%= address.postal_code%>"/>
                                                            <p class="valid_error"></p>
                                                        </div>
                                                    </div>
                                                    <textarea class="form-control" rows="3" id="" name="address" required><%= address.houseaddress %></textarea>

                                                   



                                                    <div class="mt-2 mx-1 form-group row justify-content-center">
                                                        <button type="submit" class="btn btn-md btn-black-default-hover btn-user px-5">
                                                            Save Address
                                                        </button>
                                                    </div>
                                                </form>
                                            
                                           

                        




