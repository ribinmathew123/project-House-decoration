                                </div>
                                <!-- Product Variable Single Item -->
                                <div class="d-flex align-items-center ">
                                    <div class="variable-single-item ">
                                        <div class="product-variable-quantity">
                                        </div>
                                    </div>

                                    <div class="product-add-to-cart-btn">
                                        <!-- <a href="/product/cartpage?productid=<%=productDetails._id%>" class="btn"> Add To Cart </a>  -->


                                        <a id="addCart<%= productDetails._id %>"onclick="addCart('<%=productDetails._id%>')" style="cursor: pointer;" class="btn"> Add To Cart </a>

                                            <script>

                                                function addCart(productId) {
                                                    const url = window.location.origin;
                                                    try{
                                                        fetch(`${url}/product/cartpage?productid=${productId}`, {
                                                            method: 'POST',
                                                        })
                                                    }catch(err){
                                                        alert('err')
                                                    }
                                                }
                                            </script>
                                            <script>
                                                document.getElementById("addCart<%= productDetails._id %>").addEventListener("click", function () {
                                                    Swal.fire({
                                                        title: 'Added',
                                                        text: " Added To Cart",
                                                        icon: 'success',
                                                        confirmButtonColor: '#3085d6',
                                                        confirmButtonText: 'Ok',
                                                    })
                                                });
                                            </script>
                                            <script src="https://cdn.jsdelivr.net/npm/sweetalert2@10"></script>

                                            <script
                                                src="https://cdn.jsdelivr.net/npm/popper.js@1.16.1/dist/umd/popper.min.js"></script>
